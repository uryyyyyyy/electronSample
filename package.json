{
  "name": "electron-sample",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "build": "babel src --out-dir dist",
    "build:watch": "babel src --watch --out-dir dist",
    "build:prod": "webpack --config ./webpack.config.js",
    "start": "electron ./",
    "flow": "flow",
    "package:asar": "asar pack ./ app.asar",
    "start:asar": "electron app.asar",
    "package:mac": "electron-packager ./ app --platform=darwin --arch=x64 --version=1.4.3",
    "package:linux": "electron-packager ./ app --platform=linux --arch=x64 --version=1.4.3",
    "package:win": "electron-packager ./ app --platform=win32 --arch=x64 --version=1.4.3"
  },
  "license": "MIT",
  "dependencies": {
    "babel-polyfill": "^6.20.0",
    "react": "^15.4.1",
    "react-dom": "^15.4.1"
  },
  "devDependencies": {
    "asar": "^0.12.4",
    "babel-cli": "^6.18.0",
    "babel-core": "^6.20.0",
    "babel-loader": "^6.2.10",
    "babel-preset-es2015": "^6.18.0",
    "babel-preset-react": "^6.16.0",
    "babel-register": "^6.18.0",
    "electron-packager": "^8.4.0",
    "electron-prebuilt": "^1.4.3",
    "flow-bin": "^0.37.0",
    "webpack": "^2.2.0-rc.3"
  }
}
